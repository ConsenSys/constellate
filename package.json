{
  "name": "constellate",
  "main": "lib/index.js",
  "version": "0.0.0",
  "description": "",
  "author": "Zachary Balder",
  "license": "MIT",
  "scripts": {
    "build": "babel src/ -d lib/",
    "bundle": "browserify index.js --require browserify-zlib-next:zlib > bundle.js",
    "flow": "flow --show-all-errors",
    "flow:file": "read -p 'Enter filename: ' filename && export FILENAME=$filename && flow check-contents < src/${FILENAME}",
    "gulp": "gulp",
    "remove-types:file": "read -p 'Enter filename: ' filename && export FILENAME=$filename && flow-remove-types src/${FILENAME} | js-beautify -o lib/${FILENAME}",
    "start": "http-server -a localhost -p 8888 -o -c-1",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --recursive './test/*-test.js'"
  },
  "babel": {
    "presets": [
      "es2015",
      "flow"
    ]
  },
  "dependencies": {
    "ajv": "^5.1.5",
    "bs58": "^4.0.0",
    "cids": "^0.5.0",
    "ethereumjs-util": "^5.1.1",
    "file-type": "^5.1.1",
    "ipfs": "^0.23.1",
    "ipfs-unixfs": "^0.1.11",
    "ipld-dag-cbor": "^0.11.1",
    "ipld-dag-pb": "^0.11.0",
    "multibase": "^0.3.4",
    "setimmediate": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "flow-bin": "^0.44.0",
    "flow-remove-types": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-flowtype": "^1.0.0",
    "gulp-jsbeautifier": "^2.1.0",
    "gulp-react": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "http-server": "^0.10.0",
    "mocha": "^3.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
