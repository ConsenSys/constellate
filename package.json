{
  "name": "constellate",
  "main": "lib/index.js",
  "version": "0.0.0",
  "description": "",
  "author": "Zachary Balder",
  "license": "MIT",
  "scripts": {
    "build": "babel src/ -d lib/",
    "bundle": "read -p 'Enter name of module: ' module && export MODULE=$module && browserify -t unflowify browser/${MODULE}.js --debug | exorcist bundle/${MODULE}.map.js -o bundle/${MODULE}.js",
    "flow": "flow",
    "flow:file": "read -p 'Enter path to file: ' path_to_file && export PATH_TO_FILE=$path_to_file && flow check-contents < ${PATH_TO_FILE}",
    "gulp": "gulp",
    "prepublish": "npm run build",
    "remove-types:file": "read -p 'Enter filename: ' filename && export FILENAME=$filename && flow-remove-types src/${FILENAME} > lib/${FILENAME}",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --recursive './test/*_test.js'",
    "_comment": "test command from https://github.com/Envoke-org/envokeJS/blob/master/package.json"
  },
  "babel": {
    "presets": [
      "es2015",
      "flow"
    ]
  },
  "devDependencies": {
    "ajv": "^5.0.0",
    "atob": "^2.0.3",
    "babel-cli": "^6.24.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "browserify": "^14.3.0",
    "bs58": "^4.0.0",
    "buffer": "^5.0.6",
    "chai": "^3.5.0",
    "crypto": "0.0.3",
    "eslint": "^3.19.0",
    "exorcist": "^0.4.0",
    "flow-bin": "^0.44.0",
    "flow-remove-types": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-flowtype": "^1.0.0",
    "gulp-jsbeautifier": "^2.1.0",
    "gulp-react": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "js-sha256": "^0.5.0",
    "js-sha3": "^0.5.7",
    "mocha": "^3.2.0",
    "node-fetch": "^1.6.3",
    "request": "^2.81.0",
    "tweetnacl": "^1.0.0-rc.1",
    "unflowify": "^1.0.1",
    "urlsafe-base64": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
