{
  "name": "constellate",
  "main": "index.js",
  "version": "0.0.1",
  "author": "Zachary Balder",
  "license": "MIT",
  "scripts": {
    "bundle": "browserify index.js --require browserify-zlib-next:zlib  > public/bundle.js",
    "flow": "read -p 'Enter filename: ' FILENAME && flow check-contents < src/$FILENAME",
    "remove-types": "read -p 'Enter filename: ' FILENAME && flow-remove-types src/$FILENAME | js-beautify -o lib/$FILENAME",
    "start": "node app.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "flow"
    ]
  },
  "dependencies": {
    "aes-js": "^3.1.0",
    "ajv": "^5.1.5",
    "bcryptjs": "^2.4.3",
    "bip39": "^2.3.1",
    "body-parser": "^1.17.2",
    "bs58": "^4.0.0",
    "cids": "^0.5.0",
    "ethereumjs-util": "^5.1.1",
    "ethereumjs-wallet": "^0.6.0",
    "express": "^4.15.3",
    "file-type": "^5.1.1",
    "fpcalc": "^1.2.1",
    "ipfs": "^0.24.1",
    "ipld-dag-cbor": "^0.11.1",
    "ipld-dag-pb": "^0.11.0",
    "is-buffer": "^1.1.5",
    "is-ipfs": "^0.3.0",
    "keccakjs": "^0.2.1",
    "multibase": "^0.3.4",
    "setimmediate": "^1.0.5",
    "web3": "^0.19.0",
    "xhr-request": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "benchmark": "^2.1.4",
    "browserify": "^14.4.0",
    "flow-bin": "^0.44.0",
    "flow-remove-types": "^1.2.1"
  }
}
